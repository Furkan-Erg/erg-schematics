{
  "$schema": "../../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Add UI Library to the project",
      "factory": "./ng-add/index#ngAdd"
    },
    "setup-css": {
      "description": "Add Css assets to the project",
      "factory": "./ng-add/setup#cssAdd"
    },
    "footer": {
      "description": "This schematic generates footer",
      "factory": "./footer/index#footerGenerator",
      "schema": "./footer/schema.json",
      "aliases": ["footer"]
    },
    "header": {
      "description": "This schematic generates header",
      "factory": "./header/index#headerGenerator",
      "schema": "./header/schema.json",
      "aliases": ["header"]

    },
    "login": {
      "description": "This schematic generates login",
      "factory": "./login/index#loginGenerator",
      "schema": "./login/schema.json",
      "aliases": ["login"]
    },
    "register": {
      "description": "This schematic generates register",
      "factory": "./register/index#registerGenerator",
      "schema": "./register/schema.json",
      "aliases": ["register"]
    }


  }
}
